deps=()
pkgname=zip
pkgver=3.0
src="http://downloads.sourceforge.net/infozip/zip30.tar.gz"
website=
diskusage="6.4 MB"
buildtime="0.1 SBU"
function pkgfetch {
    if [ ! -f "${sourcedir}/"$(basename $src) ]; then
        wget $src -O ${sourcedir}/$(basename $src)
    fi
    tar -xf ${sourcedir}/$(basename $src) -C $pkgdir
    }

function pkgmake {
cd $pkgdir/zip30
make -f unix/Makefile generic_gcc
}

function pkginstall {
make prefix=$installdir/usr MANDIR=$installdir/usr/share/man/man1 -f unix/Makefile install
}

function pkgafterinstall {
cd $installdir
}
