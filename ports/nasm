deps=()
pkgname=nasm
pkgver=2.11.08
src=("http://www.nasm.us/pub/nasm/releasebuilds/2.11.08/nasm-2.11.08.tar.xz")
website=
diskusage="22 MB"
buildtime="0.1 SBU"

function pkgmake {
cd $pkgdir/$pkgname-$pkgver
./configure --prefix=/usr
make
}

function pkginstall {
make DESTDIR=$installdir install
}

function pkgafterinstall {
cd $installdir
}
