deps=()
pkgname=nasm
pkgver=2.12
src=("http://www.nasm.us/pub/nasm/releasebuilds/2.12/nasm-2.12.tar.xz")
website=
diskusage="22 MB"
buildtime="0.1 SBU"

function pkgmake {
cd $pkgdir/$pkgname-$pkgver
./configure --prefix=$installdir/usr
make
}

function pkginstall {
make install
}

function pkgafterinstall {
cd $installdir
}
