deps=()
pkgname=iso-codes
pkgver=3.65
src="https://pkg-isocodes.alioth.debian.org/downloads/iso-codes-3.65.tar.xz"
website=
diskusage="111 MB (with tests)"
buildtime="0.2 SBU (with tests)"
function pkgfetch {
    if [ ! -f "${sourcedir}/"$(basename $src) ]; then
        wget $src -O ${sourcedir}/$(basename $src)
    fi
    tar -xf ${sourcedir}/$(basename $src) -C $pkgdir
    }

function pkgmake {
cd $pkgdir/$pkgname-$pkgver
./configure --prefix=/usr
make
}

function pkginstall {
make DESTDIR=$installdir install
}

function pkgafterinstall {
cd $installdir
}
